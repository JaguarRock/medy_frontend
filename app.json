{
  "expo": {
    "name": "NewSplash",
    "slug": "medy_front",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/NewSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "splash": {
        "dpi": "./assets/NewSplash.png",
        "resizeMode": "cover"
      }
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
